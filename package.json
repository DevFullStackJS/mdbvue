{
  "name": "mdbvue",
  "version": "6.6.0",
  "description": "Vue Bootstrap with Material Design",
  "author": "MDBootstrap <support@mdbootstrap.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build-lib": "node build.js",
    "build-component": "vue-cli-service build --target lib --dest dist/components/mdbDatatablePro --formats umd-min --name mdbDatatablePro ./src/components/pro/Tables/Datatable2.vue",
    "sass-compile-pro": "node-sass build/scss/mdb-pro.scss build/css/mdb.css --output-style compressed",
    "sass-compile-free": "node-sass build/scss/mdb-free.scss build/css/mdb.css --output-style compressed",
    "start": "vue-cli-service serve --open"
  },
  "main": "./lib",
  "files": [
    "lib/*",
    "nuxt/*",
    "src/components",
    "src/directives",
    "src/mixins"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "axios": "0.21.1",
    "bootstrap-css-only": "^4.4.1",
    "chart.js": "^2.8.0",
    "chartjs-plugin-datalabels": "^0.7.0",
    "core-js": "^3.1.2",
    "flush-promises": "^1.0.2",
    "moment": "^2.24.0",
    "popper.js": "^1.16.0",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.4.2",
    "vue-numeric": "^2.3.0",
    "vue-plugin-load-script": "^1.2.0",
    "vue-router": "^3.1.3",
    "vue2-perfect-scrollbar": "^1.3.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.2",
    "@vue/cli-plugin-e2e-cypress": "^4.0.2",
    "@vue/cli-plugin-eslint": "^4.0.2",
    "@vue/cli-plugin-pwa": "^4.0.2",
    "@vue/cli-plugin-unit-jest": "^4.0.2",
    "@vue/cli-service": "^4.0.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "combine-files": "^1.1.8",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "pre-commit": "^1.2.2",
    "register-service-worker": "^1.6.2",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "vee-validate": "^3.2.1",
    "vue-template-compiler": "^2.6.11"
  },
  "pre-commit": [
    "test:unit"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less)$": "<rootDir>/tests/mocks/styleMock.js"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.test.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "collectCoverage": true,
    "coverageDirectory": "tests/coverage"
  },
  "license": "SEE LICENSE IN <License.pdf>"
}
